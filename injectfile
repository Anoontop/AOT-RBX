local AnoontopLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Anoontop/AOT-RBX/main/menulib.lua')))()
local Window = AnoontopLib:MakeWindow({Name = "Anoontop Menu", HidePremium = false, SaveConfig = true, ConfigFolder = "AOT-Menu"})

local Tab = Window:MakeTab({
    Name = "Aimbot",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

